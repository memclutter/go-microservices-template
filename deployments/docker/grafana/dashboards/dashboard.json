{
  "dashboard": {
    "title": "Microservices Overview",
    "panels": [
      {
        "title": "HTTP Request Rate",
        "targets": [
          {
            "expr": "rate(microservices_http_requests_total[5m])"
          }
        ]
      },
      {
        "title": "HTTP Request Duration (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(microservices_http_request_duration_seconds_bucket[5m]))"
          }
        ]
      },
      {
        "title": "Database Query Duration (p99)",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(microservices_database_query_duration_seconds_bucket[5m]))"
          }
        ]
      },
      {
        "title": "Event Processing Rate",
        "targets": [
          {
            "expr": "rate(microservices_events_consumed_total[5m])"
          }
        ]
      }
    ]
  }
}
